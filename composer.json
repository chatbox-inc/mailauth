{
    "name": "chatbox-inc/mailauth",
    "authors": [
        {
            "name": "mikakane",
            "email": "mikakane2@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "Chatbox\\MailAuth\\": "./src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "App\\": "./sample/app/"
        },
        "classmap":[
            "./sample/database/",
            "./tests/"
        ]
    },
    "require": {
        "chatbox-inc/apiauth": "dev-master",
        "chatbox-inc/token": "0.0.1",
        "chatbox-inc/mailtoken": "0.0.*"
    },
    "require-dev": {
        "laravel/lumen": "^5.2",
        "chatbox-inc/lumen-providers": "dev-master",
        "chatbox-inc/mailclerk": "dev-master",
        "phpunit/phpunit": "^5.4",
        "peridot-php/peridot": "^1.18",
        "chatbox-inc/peridot-lumen-plugin": "^1.0"
    },
    "scripts":{
        "post-install-cmd": "touch sample/database/database.sqlite"
    },
    "config": {
        "preferred-install": {
            "chatbox-inc/*": "source"
        }
    }
}
